<emulationEnvironment xmlns="http://bwfla.bwl.de/common/datatypes">
 <id>4405</id>
 <description><title>Microsoft Windows 98 (Office)</title></description>
 <arch>i386</arch>
 <emulator bean="VirtualBox" />

<drive>
  <data></data>
  <iface>ide</iface>
  <bus>1</bus>
  <unit>0</unit>
  <type>cdrom</type>
  <filesystem>ISO</filesystem>
 </drive>

<drive>
  <data></data>
  <iface>floppy</iface>
  <bus>0</bus>
  <unit>0</unit>
  <type>floppy</type>
  <boot>false</boot>
  <filesystem>FAT12</filesystem>
 </drive>

<drive>
  <data></data>
  <iface>ide</iface>
  <bus>0</bus>
  <unit>1</unit>
  <type>disk</type>
  <boot>false</boot>
  <plugged>true</plugged>
  <filesystem>FAT16</filesystem>
 </drive>

 <drive>
  <data>binding://main_hdd</data>
  <iface>ide</iface>
  <bus>0</bus>
  <unit>0</unit>
  <type>disk</type>
  <boot>true</boot>
  <plugged>true</plugged>
 </drive>
 <binding id="main_hdd">
  <url>imagearchive:virtualbox-MS_Windows_98_Office97.vdi</url>
  <access>cow</access>
 </binding>
<nativeConfig>
setextradata $VMUUID VBoxInternal/USB/HidMouse/0/Config/CoordShift 0
</nativeConfig>   
</emulationEnvironment>
