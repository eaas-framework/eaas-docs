<emulationEnvironment xmlns="http://bwfla.bwl.de/common/datatypes">
 <id>7022</id>
 <description><title>Hatari TOS 2.06 US</title></description>
 <arch>m68k</arch>
 <emulator bean="Hatari" />

<ui_options>
<html5>
<pointer_lock>true</pointer_lock>
</html5>
</ui_options>

 <drive>
  <data></data>
  <iface>ide</iface>
  <bus>0</bus>
  <unit>1</unit>
  <type>disk</type>
  <boot>false</boot>
  <plugged>true</plugged>
  <filesystem>ZIP</filesystem>
 </drive>

 <drive>
  <data>binding://main_hdd</data>
  <iface>ide</iface>
  <bus>0</bus>
  <unit>0</unit>
  <type>disk</type>
  <boot>true</boot>
  <plugged>true</plugged>
 </drive>

 <binding id="main_hdd">
  <url>imagearchive:hatari_tOS206us.img</url>
  <access>cow</access>
 </binding>
</emulationEnvironment>
