<emulationEnvironment xmlns="http://bwfla.bwl.de/common/datatypes">
 <id>5001</id>
 <description><title>DooM</title></description>
 <arch>i386</arch>
 <emulator bean="DosBox" />

 <drive>
  <data>binding://main_hdd</data>
  <iface>ide</iface>
  <bus>0</bus>
  <unit>0</unit>
  <type>disk</type>
  <boot>true</boot>
  <plugged>true</plugged>
 </drive>
 <binding id="main_hdd">
  <url>imagearchive:doom.raw</url>
  <access>cow</access>
 </binding>
</emulationEnvironment>
